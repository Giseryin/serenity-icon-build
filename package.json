{
  "name": "serenity-icons",
  "version": "0.0.2",
  "description": "Vue3 SVG icon",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "exports": {
    ".": {
      "types": "./components/es/index.d.ts",
      "import": "./components/es/index.js",
      "require": "./components/lib/index.js"
    }
  },
  "scripts": {
    "clean": "rimraf components iconfonts png",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "build:icons": "pnpm run clean && node ./scripts/build.js",
    "build:iconfonts": "npx vi build"
  },
  "author": "frank",
  "license": "MIT",
  "files": [
    "components",
    "iconfonts"
  ],
  "devDependencies": {
    "@varlet/icon-builder": "^0.0.4",
    "@vitejs/plugin-vue": "^4.1.0",
    "css-render": "^0.15.8",
    "execa": "^5.0.0",
    "fs-extra": "^9.0.7",
    "lodash": "^4.17.21",
    "rimraf": "^5.0.1",
    "sharp": "^0.33.3",
    "typescript": "^5.0.2",
    "v2s": "^0.2.3",
    "vite": "^4.3.9",
    "vue": "^3.2.47",
    "vue-template-compiler": "2.6.12",
    "vue-tsc": "^1.4.2"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "icon",
    "vue-next",
    "vue",
    "svg",
    "serenity"
  ]
}
